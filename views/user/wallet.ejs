                                    <table id="userlist" class="display table table-striped table-hover" style="width: 100%;">
                                        <thead>
                                            <tr>
                                                <!-- <th scope="col">ID</th> -->
                                                <th scope="col">Date</th>
                                                <th scope="col">Amount</th>
                                                <th scope="col">Type</th>
                                                <th scope="col">Details</th>
                                                <th scope="col">Balance</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% if(walletHistory){ %>
                                                <% for (let i = walletHistory.length - 1; i >= 0; i--){ %>
                                                <tr>
                                                   
                                                    <td>
                                                        <%= (new Date(walletHistory[i].date)).toLocaleDateString('en-US', { month: 'long' , day: 'numeric' , year: 'numeric' })
                                                            %>
                                                    </td>
                                                    <td>
                                                        â‚¹ <%= walletHistory[i].amount %>
                                                    </td>
                                                    <td>
                                                    <%= walletHistory[i].type %>
                                                    </td>
                                                    <td>
                                                        <%= walletHistory[i].details%>
                                                    </td>
                                                    <td>
                                                    â‚¹ <%= walletHistory[i].balance %>
                                                    </td>
                                                </tr>
                                                <% } %>
                                        </tbody>
                                    </table>
                            </div>
                            <%}else{%>
