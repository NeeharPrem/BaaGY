<%- include('../layout/header.ejs') -%>
    <!-- Hero Section Begin -->
    <%if(banner && banner.length>0){%>
    <section class="hero mt-1">
        <div class="hero__slider owl-carousel">
            <a href="<%= banner[0].link %>">
                <div class="hero__items set-bg" data-setbg="/img/<%= banner[0].image %>">
                    <div class="container">
                        <div class="row">
                            <div class="col-xl-5 col-lg-7 col-md-8">
                                <div class="hero__text">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </a>
            <a href="<%= banner[1].link %>">
                <div class="hero__items set-bg" data-setbg="/img/<%= banner[1].image %>">
