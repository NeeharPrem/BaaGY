            <div class="row">
                <div class="col-lg-7 offset-lg-4">
                    <div class="banner__item">
                        <div class="banner__item__pic">
                            <% if (products && products.length> 1 && products[1].img && products[1].img.length > 0) { %>
                                <img src="img/<%= products[1].img[0] %>" alt="">
                                <% } %>
                        </div>
                        <div class="banner__item__text">
                            <h2>Travel</h2>
                            <a href="http://localhost:3000/shop?category=64ccd685bb3803b28d06907b&page=1">Shop now</a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="banner__item banner__item--middle">
                        <div class="banner__item__pic">
                            <!-- <img src="img/banner/banner-2.jpg" alt=""> -->
                        <% if (products && products.length> 1 && products[4].img && products[4].img.length > 0) { %>
                            <img src="img/<%= products[4].img[0] %>" alt="">
                            <% } %>
                        </div>
